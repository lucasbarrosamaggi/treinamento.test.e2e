{
  "name": "treinamento.test.e2e",
  "version": "1.0.0",
  "description": "Standard project with WebdriverIOv6, Cucumber, JavaScript and Appium",
  "scripts": {
    "test": "npx wdio ./config/local.conf.js --parallel=false",
    "test:parallel": "npx wdio ./config/local.conf.js --parallel=true",
    "test:multiBrowser": "DEBUG=false && npx wdio ./config/multiBrowser.conf.js --parallel=false",
    "test:debug": "export TEST_CONTEXT=funcional && DEBUG=true npx wdio ./config/local.conf.js --parallel=false",
    "test:devtools": "export TEST_CONTEXT=performance && npx wdio ./config/devtools.conf.js --parallel=false",
    "test:visual": "export TEST_CONTEXT=visual VISUAL_CONTEXT=wdio && DEBUG=true npx wdio ./config/visual.conf.js --parallel=false",
    "test:applitools": "export TEST_CONTEXT=visual VISUAL_CONTEXT=applitools && DEBUG=true npx wdio ./config/applitools.conf.js --parallel=false",
    "test:accessibility": "export TEST_CONTEXT=accessibility && DEBUG=true npx wdio ./config/local.conf.js --parallel=false",
    "android:debug": "npm run debug-app-android && wdio ./config/android.conf.js",
    "ios:debug": "TEST_IS_HEADLESS=false wdio ./config/ios.conf.js",
    "test:android": "concurrently --kill-others --raw \"node androidStart.js\" \"wdio ./config/android.conf.js\"",
    "test:ios": "TEST_IS_HEADLESS=${TEST_IS_HEADLESS:=true} && wdio ./config/ios.conf.js",
    "connect-db": "node ./tests/support/util/db/connect-modelo.js",
    "debug-app-android": "TEST_IS_HEADLESS=${TEST_IS_HEADLESS:=false} TEST_IGNORE_VIEWS=${TEST_IGNORE_VIEWS:=false} TEST_DISABLE_WATCHERS=${TEST_DISABLE_WATCHERS:=false} TEST_DISABLE_ANIMATION=${TEST_DISABLE_ANIMATION:=false}"
  },
  "keywords": [
    "webdriverio",
    "cucumber",
    "page object model",
    "Appium",
    "wdio",
    "mobile automation",
    "native apps",
    "hybrid apps",
    "newman",
    "shell",
    "parallel",
    "visual regression",
    "tedious",
    "devtools",
    "axe-core"
  ],
  "author": "Fernanda Ferreira",
  "license": "MIT",
  "devDependencies": {
    "@applitools/eyes-webdriverio": "^5.14.2",
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "@babel/runtime": "^7.10.3",
    "@rpii/wdio-html-reporter": "0.8.6",
    "@wdio/appium-service": "^6.1.0",
    "@wdio/applitools-service": "^6.0.16",
    "@wdio/cli": "^6.1.20",
    "@wdio/cucumber-framework": "^6.1.13",
    "@wdio/devtools-service": "^6.1.19",
    "@wdio/eyes.webdriverio": "^2.0.5",
    "@wdio/local-runner": "^6.1.20",
    "@wdio/mocha-framework": "^6.1.19",
    "@wdio/selenium-standalone-service": "^6.1.14",
    "@wdio/spec-reporter": "^6.1.14",
    "@wdio/sync": "^6.1.14",
    "appium": "^1.17.1",
    "axe-core": "^3.5.5",
    "axe-reports": "^1.1.11",
    "chromedriver": "^83.0.0",
    "cucumber-html-reporter": "5.2.0",
    "dotenv": "latest",
    "eslint": "^6.8.0",
    "mssql": "^6.0.1",
    "selenium-standalone": "6.17.0",
    "shelljs": "^0.8.3",
    "tedious": "6.6.5",
    "wdio-chromedriver-service": "^6.0.3",
    "wdio-cucumber-parallel-execution": "^3.6.7",
    "wdio-cucumberjs-json-reporter": "^2.0.2",
    "wdio-image-comparison-service": "^1.13.6",
    "wdio-screenshot": "^0.6.0",
    "webdriverio": "^6.1.20"
  },
  "dependencies": {
    "@types/newman": "^3.11.1",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "cucumber": "^6.0.5",
    "firebase-admin": "^9.0.0",
    "jsdom": "^16.2.2",
    "multiple-cucumber-html-reporter": "^1.16.2",
    "newman": "^5.1.2",
    "newman-reporter-htmlextra": "^1.8.6",
    "underscore": "^1.9.1"
  }
}
